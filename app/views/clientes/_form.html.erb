<%= form_for @cliente do |f| %>
  <% if @cliente.errors.any? %>
    <div id="error_explanation" class="alert alert-danger">
      <h2><%= pluralize(@cliente.errors.count, "error") %> prohibited this cliente from being saved:</h2>

      <ul>
      <% @cliente.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


	<p>
		<%= f.label :nome %>:
		<%= f.text_field :nome %>
	</p>

	<p>
		<%= f.label :cpf %>:
		<%= f.text_field :cpf %>
	</p>

	<p>
		<%= f.label :fone %>:
		<%= f.text_field :fone %>
	</p>

	<p>
		<%= f.label :data_nascimento %>:
		<%= f.text_field :data_nascimento, type: 'date' %>
	</p>

	<p>
		<%= f.label :email %>:
		<%= f.text_field :email %>
	</p>


  <%= f.submit %>
<% end %>