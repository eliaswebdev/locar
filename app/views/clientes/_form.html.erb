<%= simple_form_for @cliente do |f| %>
  <%= f.input :nome %>
  <%= f.input :cpf %>
  <%= f.input :fone %>
  <%= f.input :data_nascimento, as: :string, input_html: {type: :date} %>
  <%= f.input :email %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                clientes_path, :class => 'btn btn-default' %>
<% end %>
